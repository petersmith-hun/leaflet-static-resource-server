{
  "name": "leaflet-static-resource-server",
  "version": "0.1.0-dev",
  "bin": "./dist/lsrs-main.js",
  "scripts": {
    "start": "ts-node ./src/lsrs-main.ts",
    "diststart": "node ./dist/src/lsrs-main.js",
    "livereload": "ts-node-dev --respawn --pretty --transpile-only ./src/lsrs-main.ts",
    "test": "jest",
    "build": "tsc -p .",
    "package": "npx pkg --targets=linux --output=./build/lsrs ."
  },
  "pkg": {
    "scripts": "./dist/**/*.js"
  },
  "dependencies": {
    "config": "3.3.8",
    "cors": "2.8.5",
    "express": "4.16.1",
    "helmet": "6.0.0",
    "js-yaml": "4.1.0",
    "reflect-metadata": "0.1.13",
    "tslog": "3.3.4",
    "typedi": "0.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/config": "3.3.0",
    "@types/cors": "2.8.12",
    "@types/express":  "4.17.14",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/sinon": "10.0.13",
    "jest": "29.3.1",
    "sinon": "14.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.4"
  }
}
