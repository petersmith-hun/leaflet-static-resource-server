{
  "name": "leaflet-static-resource-server",
  "version": "1.2.0-dev",
  "bin": "./dist/lsrs-main.js",
  "scripts": {
    "start": "ts-node ./src/lsrs-main.ts",
    "diststart": "node ./dist/src/lsrs-main.js",
    "livereload": "ts-node-dev --respawn --pretty --transpile-only ./src/lsrs-main.ts",
    "test": "jest",
    "acceptance": "npx cucumber-js",
    "build": "tsc -p . && ts-node .circleci/generate-build-time.ts",
    "package": "npx pkg --targets=linux --output=./build/lsrs ."
  },
  "pkg": {
    "scripts": "./dist/**/*.js"
  },
  "dependencies": {
    "axios": "1.6.8",
    "class-validator": "0.14.1",
    "config": "3.3.11",
    "cors": "2.8.5",
    "express": "4.19.2",
    "express-oauth2-jwt-bearer": "1.6.0",
    "formidable": "3.5.1",
    "helmet": "7.1.0",
    "js-yaml": "4.1.0",
    "mysql2": "3.9.7",
    "reflect-metadata": "0.1.13",
    "remove-accents": "0.5.0",
    "sequelize": "6.37.3",
    "tslog": "4.9.2",
    "typedi": "0.10.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/preset-env": "7.24.5",
    "@babel/preset-typescript": "7.24.1",
    "@cucumber/cucumber": "10.6.0",
    "@types/config": "3.3.4",
    "@types/cors": "2.8.17",
    "@types/express":  "4.17.21",
    "@types/formidable": "3.4.5",
    "@types/jest": "29.5.12",
    "@types/mock-fs": "4.13.4",
    "@types/node": "20.12.8",
    "@types/sinon": "17.0.3",
    "@types/uuid": "9.0.8",
    "form-data": "4.0.0",
    "jest": "29.7.0",
    "mock-fs": "5.2.0",
    "oauth2-mock-server": "7.1.1",
    "sinon": "17.0.1",
    "sqlite3": "5.1.7",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  }
}
